#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules/svelte-check/bin/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules/svelte-check/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules/svelte-check/bin/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules/svelte-check/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/svelte-check@4.0.8_picomatch@2.3.1_svelte@5.2.1_typescript@5.6.3/node_modules:/Users/takashi.kato/Documents/01.work/01.dev/src/snuf-pj/ai/habit-tracker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
